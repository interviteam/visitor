{
    "name": "interactivevision/visitor",
    "description": "Support package for InteractiveVision framework.",
    "license": "MIT",
    "version": "0.1.21",
    "homepage": "https://github.com/interviteam/visitor",
    "authors": [
        {
            "name": "InteractiveVision",
            "email": "dev@interactivevision.pl"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git@gitlab.com:interactivevision/framework/globals.git"
        }
    ],
    "require-dev": {
        "interactivevision/globals": "1.x-dev"
    },
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-pcntl": "*",
        "illuminate/broadcasting": "^9.51",
        "illuminate/console": "^9.0",
        "illuminate/http": "^9.0",
        "illuminate/queue": "^9.51",
        "illuminate/support": "^9.0"
    },
    "suggest": {
        "interactivevision/globals": "Easily provide JS globals into your visitor pages."
    },
    "autoload": {
        "psr-4": {
            "InteractiveVision\\Visitor\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "InteractiveVision\\Visitor\\VisitorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
