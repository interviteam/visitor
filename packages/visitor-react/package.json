{
    "name": "@interactivevision/visitor-react",
    "version": "0.1.22",
    "description": "A simple template for a custom React component library",
    "license": "MIT",
    "homepage": "https://github.com/interviteam/visitor",
    "repository": {
        "type": "git",
        "url": "https://github.com/interviteam/visitor.git",
        "directory": "packages/visitor-react"
    },
    "bugs": {
        "url": "https://github.com/interviteam/visitor/issuess"
    },
    "type": "module",
    "module": "dist/visitor-react.js",
    "main": "dist/visitor-react.cjs",
    "types": "types/visitor-react.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "exports": {
        ".": {
            "types": "./types/visitor-react.d.js",
            "import": "./dist/visitor-react.js",
            "require": "./dist/visitor-react.cjs"
        }
    },
    "devDependencies": {
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@interactivevision/visitor": "*",
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "scripts": {
        "dev": "vite --watch",
        "clean": "rm -rf types && rm -rf dist",
        "types": "tsc --emitDeclarationOnly",
        "build": "vite build"
    }
}
