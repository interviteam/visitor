{
    "name": "@interactivevision/visitor",
    "version": "0.1.20",
    "description": "A simple template for a custom React component library",
    "license": "MIT",
    "homepage": "https://github.com/interviteam/visitor",
    "repository": {
        "type": "git",
        "url": "https://github.com/interviteam/visitor.git",
        "directory": "packages/visitor"
    },
    "bugs": {
        "url": "https://github.com/interviteam/visitor/issuess"
    },
    "type": "module",
    "module": "dist/index.esm.js",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "exports": {
        ".": {
            "types": "./types/index.d.js",
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js"
        },
        "./server": {
            "types": "./types/server.d.js",
            "import": "./dist/server.esm.js",
            "require": "./dist/server.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "types/index.d.ts"
            ],
            "server": [
                "types/server.d.ts"
            ]
        }
    },
    "scripts": {
        "dev": "./build.js --watch",
        "build": "./build.js",
        "clean": "rm -rf types && rm -rf dist",
        "types": "tsc --emitDeclarationOnly"
    },
    "devDependencies": {
        "@types/node": "^18.11.18",
        "esbuild": "^0.16.13",
        "esbuild-node-externals": "^1.6.0",
        "tslib": "^2.4.0",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "nprogress": "^0.2.0"
    }
}
